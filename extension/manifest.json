{
    "name": "InstaHealth",
    "version": "1.0.0",
    "description": "A personalized nutrition analysis and recommendation tool for instacart.",
    "manifest_version": 3,
    "author": "Abhinav Nallapa",
    "action":{
        "default_popup": "index.html",
        "default_title": "InstaHealth"
    },
    "content_scripts": [
        {
        "matches": ["https://www.instacart.com/store/publix/products/510450"],
        "js": ["script.js"],
        "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab"
    ]
}